Login;\n\n  useEffect(() => {\n    if (userResult) {\n      history.push(redirect);\n    }\n  }, [history, userResult, redirect]);\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(login(email, password));\n  };\n  return (\n    <div>\n      <FormContainer>\n        <h1>SIGN IN</h1>\n        {msg && <Message varient=\"danger\">{msg}</Message>}\n        {loading && <Loader />}\n        <Form onSubmit={submitHandler}>\n          <Form.Group controlId=\"email\">\n            <Form.Label>Email Address</Form.Label>\n            <Form.Control\n              type=\"email\"\n              placeholder=\"Enter Email\"\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n              required\n            ></Form.Control>\n          </Form.Group>\n\n          <Form.Group controlId=\"password\">\n            <Form.Label>Password</Form.Label>\n            <Form.Control\n              type=\"password\"\n              placeholder=\"Enter Your Password\"\n              value={password}\n              onChange={(e) => setPassword(e.target.value)}\n              required\n            ></Form.Control>\n          </Form.Group>\n\n          <Button type=\"submit\" varient=\"primary\">\n            SIGN-IN\n          </Button>\n        </Form>\n\n        <Row>\n          <Col>\n            New User ? &nbsp;\n            <Link to={redirect ? `register?redirect=${redirect}` : \"/register\"}>\n              Register\n            </Link>\n          </Col>\n        </Row>\n      </FormContainer>\n    </div>\n  );\n};\n\nexport default Login;\n","/home/master/Documents/herbalFinal/client/src/Components/Auth/Register.jsx",[],"/home/master/Documents/herbalFinal/client/src/Components/Pages/PrivecyPolicy.jsx",[],"/home/master/Documents/herbalFinal/client/src/Components/Pages/AboutUs.jsx",["294"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst AboutUs = () => {\n  return (\n    <div>\n      <section\n   